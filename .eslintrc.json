{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "google",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "require-jsdoc": "off"
    }
  }